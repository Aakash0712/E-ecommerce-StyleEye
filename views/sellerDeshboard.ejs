                                <div class="card">
                                    <h5 class="card-header">Products</h5>
                                    <div class="card-body p-0">
                                        <div class="table-responsive">
                                            <table class="table">
                                                <thead class="bg-light">
                                                    <tr class="border-0">
                                                        <!-- <th class="border-0">#</th> -->
                                                        <th class="border-0">Product Name</th>
                                                        <th class="border-0">Image</th>
                                                        <th class="border-0">Product Id</th>
                                                        <th class="border-0">Quantity</th>
                                                        <th class="border-0">Price</th>
                                                        <th class="border-0">Date</th>
                                                        <th class="border-0">Brand</th>
                                                        <th class="border-0"></th>
                                                        <th class="border-0"></th>
                                                    </tr>
                                                </thead>
                                                <tbody>
<% product.forEach(pro => { %>
 
                                                            <tr>    
                                                  
                                                                <% const text = pro.productName; %>
                                                                <% const maxLength = 15; %>
                                                                
                                                                
                                                                <td><%= text.length <= maxLength ? text : `${text.slice(0, maxLength)}...` %>
                                                                </td>
                                                                <td>
                                                                    <div class="m-r-10"><img src="/uplode/<%= pro.productImage %>" alt="user" class="rounded" width="45"></div>
                                                                </td>
                                                            <% const id = String(pro._id); %>
                                                            <% const idLength = 8; %>     


                                                            <td><%= id.length <= idLength ? id : `${id.slice(0, idLength)}...` %></td>
                                                            <% pro.variants.forEach(function(variant) { %>
                                                            <td><%= variant.variantsSku %></td>
                                                            <td><%= variant.variantsPrice.toLocaleString('en-US', { style: 'currency', currency: 'inr' }) %></td>
                                                            <% }) %>
                                                            <td><%= pro.date %></td>
                                                            <td><%= pro.brand %></td>
                                                            <td><span class="badge-dot badge-success  mr-2"></span><a href="/seller/product/edit/<%= pro._id %>">Edit</a></td>
                                                            <td><span class="badge-dot badge-danger  mr-2"></span> <a href="/seller/product/delete/<%= pro._id %>">Delete</a></td>
                                                        </tr>
<% }) %>                                                    

                                                   
                                                    
                                                    
                                                    <tr>
                                                        <td colspan="9"><a href="/seller/new/product" class="btn btn-outline-light float-right">Add New</a><a href="/seller/product/view" class="btn btn-outline-light float-right">View More..</a></td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                         
