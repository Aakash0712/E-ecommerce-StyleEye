                                        <h5 class="text-muted">Total Product Quantity Available</h5>
                                        <div class="metric-value d-inline-block">
                                            <h1 class="mb-1"><%= toq %></h1>
                                        </div>
                                      
                                    </div>
                                    <!-- <div id="sparkline-revenue2"></div> -->
                                </div>
                            </div>
                            <div class="col-xl-3 col-lg-6 col-md-6 col-sm-12 col-12">
                                <div class="card">
                                                    <% var lala = cart.reduce((acc, curr) => acc + curr.productQuanity, 0); %>
                                    <div class="card-body">
                                        <h5 class="text-muted">Total Quantity Sell</h5>
                                        <div class="metric-value d-inline-block">
                                            <h1 class="mb-1"><%= lala %></h1>
                                        </div>
                                       
                                    </div>
                                    <!-- <div id="sparkline-revenue3"></div> -->
