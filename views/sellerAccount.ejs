                                                        <th class="border-0">Mobile</th>
                                                        <th class="border-0">Bank Name</th>
                                                        <th class="border-0">Account Number</th>
                                                        <th class="border-0">IFSC Number</th>
                                                        <th class="border-0"></th>
                                                    </tr>
                                                </thead>
                                                <tbody>
 
                                                            <tr>    
                                                  
                                                                
                                                                <form action="/seller/account/update" method="post">
                                                                
                                                                <td><input type="text" name="firstName" value="<%= seller.firstName %>" required>
                                                                </td>
                                                                <td><input type="text" name="lastName" value="<%= seller.lastName %>" required>
                                                                </td>
                                                               <input type="hidden" name="sellerid" value="<%= seller._id %>">
                                                          
                                                            <td><input type="email" name="email" value="<%= seller.email %>" required></td>
                                                            <td><input type="number" name="mobile" value="<%= seller.mobile %>" required></td>
                                                            <td><input type="text" name="bankAccount[bankName]" value="<%= seller.bankAccount.bankName %>" required></td>
         
                                                            <td> <input type="number" id="accountNumber" name="bankAccount[accountNumber]" value="<%= seller.bankAccount.accountNumber %>" required></td>
          
                                                            <td><input type="text" id="routingNumber" name="bankAccount[ifscNumber]" value="<%= seller.bankAccount.ifscNumber %>" required></td>
                                                            <td><span class="badge-dot badge-success  mr-2"></span><button type="submit" name="update" value="seller">Update</button></td>
                                                        </tr>
                                                        
                                                   
                                                    
                                                    
                                                  
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-xl-9 col-lg-12 col-md-6 col-sm-12 col-12">
                                <div class="card">
                                    <h5 class="card-header">Products</h5>
                                    <div class="card-body p-0">
                                        <div class="table-responsive">
                                            <table class="table">
                                                <thead class="bg-light">
                                                    <tr class="border-0">
                                                        <!-- <th class="border-0">#</th> -->
                                                        <th class="border-0">Shop Name</th>
                                                        <th class="border-0">Shop Address</th>
                                                        <th class="border-0">Shop City</th>
                                                        <th class="border-0">Shop State</th>
                                                        <th class="border-0">Shop Pincode</th>
                                                        
                                                        <th class="border-0"></th>
                                                    </tr>
                                                </thead>
                                                <tbody>
 
                                                            <tr>    
                                                  
                                                                
                                                                
            
            <td><input id="name" type="text" name="shopName" value="<%= shop.shopName %>" required/>
                                                                </td>
                                                               <input type="hidden" name="shopid" value="<%= shop._id %>">
                                                            <td><input type="text" id="name" name="address[street]" value="<%= shop.address.street %>" required></td>
                                                            <td><input type="text" id="accountNumber" name="address[city]" value="<%= shop.address.city %>" required></td>
                                                            <td><input type="text" id="routingNumber" name="address[state]" value="<%= shop.address.state  %>" required></td>
                                                            <td><input type="number" id="routingNumber" name="address[zip]" value="<%= shop.address.zip %>" required></td>
                                                            <td><span class="badge-dot badge-success  mr-2"></span><button type="submit" name="update" value="shop">Update</button></td>
                                                            <!-- <td><span class="badge-dot badge-danger  mr-2"></span> <a href="#">Delete</a></td> -->
                                                        </tr>
                                                    </form>

                                                   
                                                    
                                                    
                                                 
                                                </tbody>
                                            </table>
