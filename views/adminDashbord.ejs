    }
    
    .action-button {
      border-radius: 4px;
      height: 32px;
      background-color: var(--app-content-secondary-color);
      border: 1px solid var(--app-content-secondary-color);
      display: flex;
      align-items: center;
      color: var(--app-content-main-color);
      font-size: 14px;
      margin-left: 8px;
      cursor: pointer;
